{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/admin(.*)",
            "destination": "/admin/index.html"
        },
        {
            "source": "/partner(.*)",
            "destination": "/partner/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}